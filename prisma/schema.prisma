generator client {
  provider      = "prisma-client-rust"
  output        = "../src/db.rs"
  binaryTargets = ["native"]
}

generator erd {
  provider = "prisma-erd-generator"
  output   = "./erd.svg"
}

generator dbml {
  provider   = "prisma-dbml-generator"
  outputName = "dev.dbml"
}

generator docs {
  provider = "node node_modules/prisma-docs-generator"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id          String @id
  displayName String
}
